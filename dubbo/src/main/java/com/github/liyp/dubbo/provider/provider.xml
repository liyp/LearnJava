<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="say-hello-server" />
    <dubbo:registry address="224.5.6.7:1234" protocol="multicast" />
    <dubbo:protocol name="dubbo" port="20880" />
    <dubbo:service interface="com.github.liyp.dubbo.api.HelloService" ref="serviceImpl" />
    <bean id="serviceImpl" class="com.github.liyp.dubbo.provider.HelloServiceImpl" />

</beans>